@CONFERENCE{Kojouharov202411691,
	author = {Kojouharov, Velin H. and Wang, Tianyu and Fernandez, Matthew and Maeng, Jiyeon and Goldman, Daniel I.},
	title = {Anisotropic body compliance facilitates robotic sidewinding in complex environments},
	year = {2024},
	journal = {Proceedings - IEEE International Conference on Robotics and Automation},
	pages = {11691 - 11697},
	doi = {10.1109/ICRA57147.2024.10611625},
